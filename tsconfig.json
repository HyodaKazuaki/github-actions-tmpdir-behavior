{
    "compilerOptions": {
      "target": "es2022",
      "module": "ES2022",
      "moduleResolution": "Bundler",
      "baseUrl": "./",
      "paths": {
        "@/*": ["./src/*"]
      },
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true
    }
  }